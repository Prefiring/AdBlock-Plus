{
    "manifest_version": 3,
    "name": "AdBlock Plus",
    "version": "1.2.0",
    "description": "Blocks ads on various sites...",
    "permissions": [
        "declarativeNetRequest"
    ],
    "declarative_net_request": {
        "rule_resources": [
            {
                "id": "ruleset_1",
                "enabled": true,
                "path": "rules.json"
            }
        ]
    },
    "icons": {
        "16": "/icons/16x16.png",
        "48": "/icons/48x48.png",
        "128": "/icons/128x128.png"
    }
}